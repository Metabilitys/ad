--Kill
function kill(msg)
    local dead = string.sub(msg, 11)
while death == true do
wait(0.0000000001)
game:GetService'Players':Chat(("kill "..dead))
end
end


game.Players.LocalPlayer.Chatted:connect(function(msg)

if string.sub(msg, 1, 9) == ";spamkill" then
death = true
kill(msg)
end

if string.sub(msg, 1, 5) == ";stop" then
death = false
end
end)
